{
  "name": "licenseathon-vue",
  "version": "1.0.0",
  "description": "Layouts for Licenseathon 2023",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tempystral/licenseathon-2023"
  },
  "license": "MIT",
  "author": "tempystral",
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics",
    "autofix:extension": "eslint --fix --ext .ts src/extension && eslint --fix --ext .d.ts src/types",
    "build": "run-s clean build:*",
    "build:browser": "vite build",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "rm -rf node_modules/.cache dashboard graphics extension shared/dist",
    "schema-types": "nodecg schema-types",
    "start": "nodecg start",
    "watch": "run-p watch:*",
    "watch:browser": "vite",
    "watch:extension": "tsc -b tsconfig.extension.json -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "fitty": "^2.4.2",
    "nodecg": "^2.5.3",
    "nodecg-vue-composable": "~/git/nodecg-vue-composable",
    "iframe-resizer": "^5.4.5",
    "module-alias": "^2.2.3",
    "tailwindcss": "^4.1.4",
    "tailwindcss-primeui": "^0.6.1",
    "@mdi/js": "^7.4.47",
    "@primeuix/themes": "^1.0.3",
    "@jamescoyle/vue-icon": "^0.1.2"
  },
  "devDependencies": {
    "@nodecg/types": "^2.5.3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vueuse/head": "^2.0.0",
    "@tailwindcss/vite": "^4.1.4",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.10.5",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.86.3",
    "primevue": "^4.3.3",
    "speedcontrol-util": "^0.6.1",
    "trash-cli": "^6.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.2",
    "vite-plugin-nodecg": "^2.1.0",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-inline-svg": "^4.0.0",
    "vue-tsc": "^2.2.10"
  },
  "nodecg": {
    "compatibleRange": "^2.5.3",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.5.0"
    },
    "dashboardPanels": [
      {
        "name": "commentator-panel",
        "title": "Commentators",
        "file": "commentatorPanel.html",
        "width": 3
      }
    ],
    "graphics": [
      {
        "file": "splash.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "setup.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "credits.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "solo_16_9.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "solo_4_3.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "solo_GBA.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vaadin/vaadin-usage-statistics",
      "better-sqlite3",
      "esbuild",
      "iframe-resizer"
    ]
  }
}